		<query>
			<sql><![CDATA[select sum(temp_size) MB_siz,tablespace,username,schemaname,count(*),instance
from (
SELECT b.tablespace, a.module,client_info,
       ROUND(((b.blocks*p.value)/1024/1024),2) AS temp_size,
       a.inst_id as Instance,
       a.sid||','||a.serial# AS sid_serial,
       NVL(a.username, '(oracle)') AS username,schemaname,
       a.program,
       a.status,
       a.sql_id
FROM   gv$session a,
       gv$sort_usage b,
       gv$parameter p
WHERE  p.name  = 'db_block_size'
AND    a.saddr = b.session_addr
AND    a.inst_id=b.inst_id
AND    a.inst_id=p.inst_id
and a.module =:MODULE
and a.client_info =:CLIENT_INFO
)
group by tablespace,instance,username,schemaname
order by 1 desc]]></sql>
