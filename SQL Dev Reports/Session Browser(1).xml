	<name><![CDATA[Session Browser]]></name>
	<description><![CDATA[Active Session Information

Version 1.0]]></description>
	<tooltip><![CDATA[Active Sessions]]></tooltip>
	<drillclass><![CDATA[null]]></drillclass>
	<CustomValues>
		<TYPE>horizontal</TYPE>
	</CustomValues>
	<query>
		<sql><![CDATA[select * from 
(select s.inst_id,s.sid,s.saddr,s.serial#,s.program,s.module,s.event,s.SQL_ID,s.osuser,s.schemaname,s.username,s.machine,s.logon_time
,trunc((sysdate- s.SQL_EXEC_START)*24*60,2) Mins_Exec
,trunc((sysdate-s.logon_time)*24*60,2) Mins_Logon
,s.seconds_in_wait
, s.final_blocking_session,s.final_blocking_instance
/*,t.sql_text*/

,s.P1, s.P1Text, s.P2, s.P2Text, s.P3, s.P3Text
,s.client_info,s.action
,s.sql_hash_value
, 'ALTER SYSTEM KILL SESSION '''|| SID || ',' || s.SERIAL# || ', @' || s.INST_ID || ''';' Kill
from gv$session s 
/*left outer join  gv$sqltext t 
on ( t.hash_value = s.sql_hash_value and t.inst_id=s.inst_id) */
where 
s.type!='BACKGROUND'
--and s.status='ACTIVE' and s.sql_id is not null
/*and nvl(t.piece,0)<1 */
)
order by mins_exec desc nulls last, mins_logon desc nulls last]]></sql>
	</query>
