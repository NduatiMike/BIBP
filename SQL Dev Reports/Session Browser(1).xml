	<display id="null" type="" style="Table" enable="true">
		<name><![CDATA[LongOps]]></name>
		<description><![CDATA[% done of long running transactions]]></description>
		<tooltip><![CDATA[Long operations]]></tooltip>
		<drillclass><![CDATA[null]]></drillclass>
		<CustomValues>
			<TYPE>horizontal</TYPE>
		</CustomValues>
		<query>
			<sql><![CDATA[SELECT to_char(Start_Time, 'yyyy-mm-dd HH24:mi:ss') Start_Time, Message, Units, TotalWork, SoFar, ROUND(sl.sofar/sl.totalwork*100, 2) progress_pct,
       lpad(TRUNC(sl.elapsed_seconds/60),2,'0') || ':' || lpad(MOD(sl.elapsed_seconds,60),2,'0') elapsed,
       lpad(TRUNC(sl.time_remaining/60),2,'0')  || ':' || lpad(MOD(sl.time_remaining,60),2,'0') remaining,
       inst_id
FROM         gv$session_longops sl
WHERE  SID =:SID
AND    INST_ID = :INST_ID
ORDER BY Start_Time DESC]]></sql>
